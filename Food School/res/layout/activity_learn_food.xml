<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/TableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LearnFoodActivity"
        android:background="@drawable/back"
     >

    <!-- Replace with image buttons -->
   <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp" >
       <ImageButton
            android:id="@+id/imageButtona"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:scaleType="fitStart"
            android:layout_weight="10"
            android:src="@drawable/img_navigation_back_english"
            android:background="@android:color/transparent"/>
       <ImageButton
            android:id="@+id/imageButtonb"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:scaleType="fitCenter"
            android:layout_weight="4"
            android:src="@drawable/img_navigation_grid_button"
            android:background="@android:color/transparent"/>
        
        <ImageButton
            android:id="@+id/imageButtonc"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:scaleType="fitStart"
            android:layout_weight="2"
            android:src="@drawable/img_navigation_next_english"
            android:background="@android:color/transparent"/>
        <ImageButton
            android:id="@+id/imageButtond"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:scaleType="fitEnd"
            android:layout_weight="1"
            android:src="@drawable/img_navigation_home_english"
            android:background="@android:color/transparent"/>        
    </TableRow>
        
    <TableRow
        android:id="@+id/tableRow1"
